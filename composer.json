{
    "name": "ccasanovas/cake-s3upload",
    "description": "S3UploadSDK plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "4.*",
        "josegonzalez/cakephp-upload": "^4.0",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Ccasanovas\\S3UploadSDK\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Ccasanovas\\S3UploadSDK\\Test\\": "tests",
            "Ccasanovas\\S3UploadSDK\\Test\\Fixture\\": "tests"
        }
    }
}
